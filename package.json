{
  "name": "fast_food",
  "version": "1.0.0",
  "description": "Web application for ordering food",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./routes --fix",
    "test": "nyc mocha ./dist/test --exit",
     "build": "rm -rf dist/ && babel ./server --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log",
    "start": "node dist/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dropTable": "node ./dist/db/dropTable.js",
    "createTable": "node ./dist/db/table.js",
    "defaultAdmin": "node ./dist/db/defaultadmin.js",
    "table": "npm run dropTable && npm run createTable && npm run defaultAdmin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Danielshow/Fast-Food"
  },
  "author": "Danielshow",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "multer": "^1.3.1",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
